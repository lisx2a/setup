syntax on
let g:solarized_termcolors=256
set t_Co=256
set background=dark
colorscheme solarized
set softtabstop=2
set tabstop=2
set hlsearch
setlocal nonumber "hides lines number set by janus
set ignorecase
set scrolloff=10
set ruler
map <C-d>     :NERDTreeToggle<CR>
let g:cssColorVimDoNotMessMyUpdatetime = 1
let g:closetag_html_style=1
source ~/.janus/closetag.vim
let g:python_folding = 1
